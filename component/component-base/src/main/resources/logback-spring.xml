<?xml version="1.0" encoding="UTF-8"?><configuration>    <!--spring boot admin log level管理-->    <!--<include resource="org/springframework/boot/logging/logback/base.xml"/>-->    <jmxConfigurator/>    <springProperty scope="context" name="appName" source="spring.application.name"/>    <springProperty scope="context" name="activeProfile" source="spring.profiles.active"/>    <springProfile name="local">        <property name="LOG_PATH" value="D:/cloudlogs"/>    </springProfile>    <springProfile name="dev">        <property name="LOG_PATH" value="/yimao/applog"/>    </springProfile>    <springProfile name="test">        <property name="LOG_PATH" value="/yimao/applog"/>    </springProfile>    <springProfile name="pro">        <property name="LOG_PATH" value="/data/jarlog"/>    </springProfile>    <property name="file.prefix" value="${LOG_PATH}/${appName}"/>    <conversionRule conversionWord="clr" converterClass="org.springframework.boot.logging.logback.ColorConverter"/>    <conversionRule conversionWord="wex" converterClass="org.springframework.boot.logging.logback.WhitespaceThrowableProxyConverter"/>    <conversionRule conversionWord="wEx" converterClass="org.springframework.boot.logging.logback.ExtendedWhitespaceThrowableProxyConverter"/>    <property name="FILE_LOG_PATTERN"              value="${FILE_LOG_PATTERN:-%d{yyyy-MM-dd HH:mm:ss.SSS} ${LOG_LEVEL_PATTERN:-%5p} ${PID:- } --- [%t] %-40.40logger{39} : %m%n${LOG_EXCEPTION_CONVERSION_WORD:-%wEx}}"/>    <property name="CONSOLE_LOG_PATTERN"              value="${CONSOLE_LOG_PATTERN:-%clr(%d{yyyy-MM-dd HH:mm:ss.SSS}){faint} %clr(${LOG_LEVEL_PATTERN:-%5p}) %clr(${PID:- }){magenta} %clr(---){faint} %clr([%15.15t]){faint} %clr(%-40.40logger{39}){cyan} %clr(:){faint} %m%n${LOG_EXCEPTION_CONVERSION_WORD:-%wEx}}"/>    <appender name="info" class="ch.qos.logback.core.rolling.RollingFileAppender">        <File>${file.prefix}/info.log</File>        <encoder>            <pattern>${FILE_LOG_PATTERN}</pattern>        </encoder>        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">            <fileNamePattern>${file.prefix}/info-%d{yyyy-MM-dd}.log</fileNamePattern>            <maxHistory>7</maxHistory>        </rollingPolicy>        <!--<triggeringPolicy class="ch.qos.logback.core.rolling.SizeBasedTriggeringPolicy">-->        <!--<MaxFileSize>10MB</MaxFileSize>-->        <!--</triggeringPolicy>-->        <filter class="ch.qos.logback.classic.filter.LevelFilter">            <level>INFO</level>            <onMatch>ACCEPT</onMatch>            <onMismatch>DENY</onMismatch>        </filter>    </appender>    <appender name="error" class="ch.qos.logback.core.rolling.RollingFileAppender">        <File>${file.prefix}/error.log</File>        <encoder>            <pattern>${FILE_LOG_PATTERN}</pattern>        </encoder>        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">            <fileNamePattern>${file.prefix}/error-%d{yyyy-MM-dd}.log</fileNamePattern>            <maxHistory>7</maxHistory>        </rollingPolicy>        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">            <level>WARN</level>        </filter>    </appender>    <appender name="STDOUT-DEBUG" class="ch.qos.logback.core.ConsoleAppender">        <encoder>            <pattern>${CONSOLE_LOG_PATTERN}</pattern>            <charset>utf8</charset>        </encoder>        <filter class="ch.qos.logback.classic.filter.LevelFilter">            <level>DEBUG</level>            <onMatch>ACCEPT</onMatch>            <onMismatch>DENY</onMismatch>        </filter>    </appender>    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">        <encoder>            <pattern>${CONSOLE_LOG_PATTERN}</pattern>            <charset>utf8</charset>        </encoder>        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">            <level>DEBUG</level>        </filter>    </appender>    <appender name="logstash-tcp" class="net.logstash.logback.appender.LogstashTcpSocketAppender">        <!--<destination>47.100.244.34:10040</destination>-->        <remoteHost>47.100.244.34</remoteHost>        <port>10040</port>        <encoder charset="UTF-8" class="net.logstash.logback.encoder.LogstashEncoder"/>        <writeBufferSize>200000</writeBufferSize>        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">            <!--过滤掉TRACE和DEBUG级别的日志-->            <level>INFO</level>        </filter>    </appender>    <!--<logger name="com.hmily.tcc.bonuspoint" level="DEBUG"/>-->    <!--<logger name="com.hmily.tcc.lottery" level="DEBUG"/>-->    <!--<logger name="com.hmily.tcc" level="DEBUG"/>-->    <!--在使用@ApiModelProperty注解在字段上时，如果字段的类型为Long或是int类型，那么程序启动后，访问swagger-ui.html的页面，程序会报错-->    <!--在swagger的官方文档中介绍是一个bug；可以忽略这个错误，如果看着不爽，可以调整日志的级别-->    <logger name="io.swagger.models.parameters.AbstractSerializableParameter" level="ERROR"/>    <!--开发环境下开启com.yimao包下的DEBUG模式-->    <springProfile name="local">        <logger name="com.yimao" level="DEBUG"/>    </springProfile>    <root level="info">        <springProfile name="local">            <!--<appender-ref ref="STDOUT-DEBUG"/>-->            <appender-ref ref="STDOUT"/>        </springProfile>        <springProfile name="dev,test">            <appender-ref ref="info"/>            <appender-ref ref="error"/>            <appender-ref ref="logstash-tcp"/>        </springProfile>        <springProfile name="pro">            <appender-ref ref="info"/>            <appender-ref ref="error"/>            <appender-ref ref="logstash-tcp"/>        </springProfile>    </root></configuration>